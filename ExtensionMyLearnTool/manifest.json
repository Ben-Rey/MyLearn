{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_page": "options.html",

  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/orca16.png",
      "32": "images/orca32.png",
      "48": "images/orca64.png",
      "128": "images/orca128.png"
    }
  },
  "icons": {
    "16": "images/orca16.png",
    "32": "images/orca32.png",
    "48": "images/orca64.png",
    "128": "images/orca128.png"
  },
  "manifest_version": 2
}
